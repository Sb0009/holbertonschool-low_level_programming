#include <stdio.h>
/**
 *#include is a preprocessor command  -
 *stdio.h is standard input and output file in the program
 * stdio.h contains functions printf to take the input and display outpout
 *Return: 0 is the statement for ending the program (exit status)
 */

int main (void)
{
  printf("Hello World");
  return 0;
}
